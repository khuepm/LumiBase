{
  "name": "lumibase",
  "version": "1.0.0",
  "description": "The Ultimate Launchpad for Modern Apps. A premium starter-kit combining Firebase, Supabase, and Directus.",
  "main": "index.js",
  "scripts": {
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:property": "vitest run tests/property",
    "test:coverage": "vitest run --coverage",
    "test:env:up": "docker-compose -f docker-compose.test.yml up -d",
    "test:env:down": "docker-compose -f docker-compose.test.yml down",
    "test:env:clean": "docker-compose -f docker-compose.test.yml down -v",
    "test:env:logs": "docker-compose -f docker-compose.test.yml logs -f",
    "test:env:status": "docker-compose -f docker-compose.test.yml ps",
    "test:with-env": "npm run test:env:up && npm test && npm run test:env:down",
    "seed-data": "tsx scripts/seed-data.ts"
  },
  "keywords": [
    "firebase",
    "supabase",
    "directus",
    "cms",
    "authentication",
    "postgresql",
    "starter-kit",
    "boilerplate"
  ],
  "author": "Khue Pham",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@supabase/supabase-js": "^2.95.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.16.0",
    "dotenv": "^16.3.1",
    "fast-check": "^4.5.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.18.0",
    "supabase": "^2.75.5",
    "tsx": "^4.7.0",
    "vitest": "^1.0.0",
    "yaml": "^2.3.4"
  }
}
